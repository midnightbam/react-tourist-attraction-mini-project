{
  "version": 2,
  "buildCommand": "echo 'Build complete'",
  "builds": [
    {
      "src": "server/app.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "server/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
